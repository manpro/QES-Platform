{
  "dashboard": {
    "id": null,
    "title": "QES Platform - Business Overview",
    "tags": ["qes", "business", "metrics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "query": "label_values(qes_usage_total, tenant_id)",
          "current": {
            "value": "all",
            "text": "All Tenants"
          },
          "options": [],
          "includeAll": true,
          "multi": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Revenue Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(qes_revenue_total)",
            "refId": "A",
            "legendFormat": "Total Revenue"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Subscriptions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(qes_subscriptions_active)",
            "refId": "A",
            "legendFormat": "Active Subscriptions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Users (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "qes_users_active{time_period=\"24h\"}",
            "refId": "A",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "100 - avg(qes_error_rate)",
            "refId": "A",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds",
              "thresholds": {
                "steps": [
                  {
                    "color": "red",
                    "value": 0
                  },
                  {
                    "color": "yellow",
                    "value": 95
                  },
                  {
                    "color": "green",
                    "value": 99
                  }
                ]
              }
            },
            "custom": {
              "displayMode": "basic"
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Usage by Metric Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (metric_type) (rate(qes_usage_total[$__rate_interval]))",
            "refId": "A",
            "legendFormat": "{{metric_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Revenue by Plan Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (plan_type) (qes_revenue_total)",
            "refId": "A",
            "legendFormat": "{{plan_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(qes_requests_total[$__rate_interval]))",
            "refId": "A",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(qes_error_rate)",
            "refId": "A",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 17
        }
      },
      {
        "id": 9,
        "title": "Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(qes_request_duration_seconds_bucket[$__rate_interval])) by (le))",
            "refId": "A",
            "legendFormat": "95th Percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(qes_request_duration_seconds_bucket[$__rate_interval])) by (le))",
            "refId": "B",
            "legendFormat": "Median"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 17
        }
      }
    ]
  }
}